{
    "ProblemType": "Incompressible",
	"Remeshing": {
		"hchar": 0.1,
		"alpha": 1.1,
		"omega": 0.7,
		"gamma": 0.7,
		"boundingBox": [-1, -1, -1, 1, 1, 100]
	},
	"Solver": {
		"gravity": 9.81,
		"Time": {
			"adaptDT": true,
    		"coeffDTincrease": 1.5,
    		"coeffDTdecrease": 2.0,
    		"maxDT": 0.001,
			"initialDT": 1e-8,
    		"endTime": 4
		},
		"Picard": {
			"relTol": 5e-6,
    		"maxIter": 10
		},
		"Fluid": {
			"rho": 1000,
			"mu": 1e-3,		
		},
		"initialCondition" : [0, 0, 0, 0],
		"Extractors" : [
			{
				"type": "GMSH",
				"outputFile": "results.msh",
                "timeBetweenWriting": 0.01,
                "whatToWrite": ["rho", "p", "ke"],
				"writeAs": "NodesElements"
			}
		]
	},
	"verboseOutput": true
}
